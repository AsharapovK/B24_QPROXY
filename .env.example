# Основные настройки
NODE_ENV=development
PROXY_PORT=3000
SERVER_IP=127.0.0.1

# URL целевых API
TARGET_BASE_URL="https://script.google.com/macros/s/.../exec"
TARGET_BASE_URL_INVOICE="https://script.google.com/macros/s/.../exec"

# Настройки очереди
MAX_CONCURRENCY=1
MAX_RETRIES=3
REQUEST_TIMEOUT=70000

# Настройки логирования
LOG_LEVEL=info
LOG_FILE=app.log

# Настройки безопасности
RATE_LIMIT_WINDOW_MS=900000  # 15 минут
RATE_LIMIT_MAX_REQUESTS=100  # Максимальное количество запросов за окно
